{
	"version": 2,
	"builds": [
	  {
		"src": "dist/index.js",
		"use": "@now/node-server"
	  }
	],
	"routes": [
	  {
		"src": "/api/user/sign_up",
		"dest": "dist/index.js"
	  },
	  {
		"src": "/api/user/sign_in",
		"dest": "dist/index.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "dist/index.js"
	  }
	],
	"env": {
	  "NODE_ENV": "production"
	}
}
